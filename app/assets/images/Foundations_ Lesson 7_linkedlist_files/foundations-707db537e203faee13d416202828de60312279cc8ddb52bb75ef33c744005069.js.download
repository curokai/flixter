function submitForm(e){$("#challenge_submission_form").submit(function(s){s.preventDefault();var o=CKEDITOR.instances.message&&CKEDITOR.instances.message.getData(),n={coding_challenge_submission:{challenge_name:$("#challenge_name").val(),challenge_solution:$("#challenge_solution").val(),github_url:$("#github_url").val(),message:o}};$.post(e,n).success(function(){$(".challenge-submission").hide(),$(".challenge-resources").show(),$("body").scrollTo(".challenge-resources",{offset:-200}),$("#challenge-resources").focus(),$(".pre-submit").hide(),$(".post-submit").removeClass("hide"),$("#post-submit-show").focus()}).fail(function(e){var s=e.responseJSON.errors;Object.keys(s).forEach(function(e){$(".error_"+e).text(s[e][0]).attr("aria-describedby","error_"+e),$("#error_"+e).focus()})})})}